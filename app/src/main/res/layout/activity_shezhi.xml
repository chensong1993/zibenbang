        <RelativeLayout
            android:id="@+id/rl_clear"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:background="@color/white">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/space"
                android:text="@string/x_clear_cache"
                android:textColor="@color/black"
                android:textSize="14dp" />

            <ImageView
                android:id="@+id/img_icon_clear"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:background="@mipmap/ic_keyboard_arrow_right_black_36dp"
                android:backgroundTint="@color/hui7" />


            <TextView
                android:id="@+id/tv_clear"
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:layout_toLeftOf="@id/img_icon_clear"
                android:gravity="center_vertical"
                android:text=""
                android:textColor="@color/hui2"
                android:textSize="14dp" />
        </RelativeLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="12dp"
