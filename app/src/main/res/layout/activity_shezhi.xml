                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:background="@color/white"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/space"
                android:text="@string/x_guanyu"
                android:textColor="@color/black"
                android:textSize="14dp" />

            <ImageView
                android:id="@+id/img_icon_about"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:background="@mipmap/ic_keyboard_arrow_right_black_36dp"
                android:backgroundTint="@color/hui7" />

        </RelativeLayout>


        <TextView
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/hui" />

        <RelativeLayout
            android:id="@+id/rl_business"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:background="@color/white">

            <TextView
                android:id="@+id/tv_business"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:background="@color/white"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/space"
                android:text="@string/x_shangwu"
                android:textColor="@color/black"
                android:textSize="14dp" />

            <ImageView
                android:id="@+id/img_icon_business"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:background="@mipmap/ic_keyboard_arrow_right_black_36dp"
                android:backgroundTint="@color/hui7" />

        </RelativeLayout>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="12dp"
            android:background="@color/hui" />

        <RelativeLayout
            android:id="@+id/rl_version"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white">

            <TextView
                android:id="@+id/tv_version"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/space"
                android:text="检测版本"
                android:paddingTop="10dp"
                android:paddingBottom="5dp"
                android:textColor="@color/black"
                android:textSize="14dp" />

            <TextView
                android:id="@+id/tv_bb"
                android:layout_below="@+id/tv_version"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/space"
                android:text="@string/x_versions"
                android:textColor="@color/black"
                android:paddingBottom="10dp"
                android:textSize="14dp" />

            <TextView
                android:layout_below="@+id/tv_version"
                android:id="@+id/tv_versions"
                android:text=""
                android:layout_toRightOf="@id/tv_bb"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:paddingBottom="10dp"
                android:textSize="14dp" />
            <ImageView
                android:id="@+id/img_message"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/ic_message"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/space"
                android:layout_centerVertical="true"
                android:visibility="gone"/>
        </RelativeLayout>


